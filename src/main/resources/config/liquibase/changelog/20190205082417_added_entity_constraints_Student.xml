<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20190205082417-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="student"
                                 constraintName="fk_student_department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

        <addForeignKeyConstraint baseColumnNames="batch_id"
                                 baseTableName="student"
                                 constraintName="fk_student_batch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="batch"/>

        <addForeignKeyConstraint baseColumnNames="section_id"
                                 baseTableName="student"
                                 constraintName="fk_student_section_id"
                                 referencedColumnNames="id"
                                 referencedTableName="section"/>

        <addForeignKeyConstraint baseColumnNames="branch_id"
                                 baseTableName="student"
                                 constraintName="fk_student_branch_id"
                                 referencedColumnNames="id"
                                 referencedTableName="branch"/>

    </changeSet>
</databaseChangeLog>
